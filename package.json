{
  "name": "react-google-calendar-events-list",
  "description": "Quick boilerplate for browserify and React modules.",
  "main": "index.jsx",
  "dependencies": {
    "react": "~0.8"
  },
  "devDependencies": {
    "browserify": "~2.36.0",
    "envify": "~0.2.0",
    "reactify": "~0.4.0",
    "statics": "~0.1.0",
    "superagent": "^0.18.0",
    "uglifyjs": "~2.3.6",
    "watchify": "~0.4.1"
